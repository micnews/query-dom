{
  "name": "query-dom",
  "version": "2.0.2",
  "description": "query-focused & somewhat standards compliant subset of the DOM",
  "main": "dist/lib/index.js",
  "scripts": {
    "test": "semistandard | snazzy && nyc ava",
    "benchmark:build": "mkdir -p dist && babel benchmark --out-dir dist/benchmark",
    "benchmark": "npm-run-all --parallel benchmark:build build && node dist/benchmark/benchmark.js",
    "build": "babel lib --out-dir dist/lib",
    "prepublish": "rm -r dist && npm run build",
    "posttest": "readme package.json --travis > readme.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micnews/query-dom.git"
  },
  "author": "mic.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micnews/query-dom/issues"
  },
  "homepage": "https://github.com/micnews/query-dom#readme",
  "devDependencies": {
    "ava": "^0.11.0",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "jsdom": "^8.0.1",
    "minidom": "^1.0.0",
    "npm-run-all": "^1.5.1",
    "nyc": "^5.5.0",
    "semistandard": "^7.0.5",
    "snazzy": "^2.0.1",
    "tsml": "^1.0.1"
  },
  "dependencies": {
    "camelcase-css": "^1.0.0",
    "es6-set": "^0.1.4",
    "object-assign": "^4.0.1",
    "parse5": "^2.1.5",
    "to-fast-properties": "^1.0.1"
  }
